    "symbolIcon.keywordForeground": "#a78bfa",
    "symbolIcon.stringForeground": "#86efac"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#4a4a60", "fontStyle": "italic" }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": { "foreground": "#a78bfa" }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": { "foreground": "#86efac" }
    },
    {
      "name": "Template literal punctuation",
      "scope": ["string.template punctuation.definition.string"],
      "settings": { "foreground": "#86efac" }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": { "foreground": "#67e8f9" }
    },
    {
      "name": "Types & Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "entity.name.namespace",
        "entity.name.tag"
      ],
      "settings": { "foreground": "#fde68a" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.numeric.integer", "constant.numeric.float"],
      "settings": { "foreground": "#fb923c" }
    },
    {
      "name": "Constants & Booleans",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": { "foreground": "#fb923c" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#f0f0f5" }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "variable.parameter"
      ],
      "settings": { "foreground": "#f0f0f5" }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "meta.property-name",
        "support.type.property-name"
      ],
      "settings": { "foreground": "#67e8f9" }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#f87171" }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#fde68a" }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css", "meta.property-name.css"],
      "settings": { "foreground": "#67e8f9" }
    },
    {
      "name": "CSS Values",
      "scope": ["meta.property-value.css", "support.constant.property-value.css"],
      "settings": { "foreground": "#86efac" }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": { "foreground": "#fb923c" }
    },
    {
      "name": "Escape sequences",
      "scope": ["constant.character.escape"],
      "settings": { "foreground": "#fb923c" }
    },
    {
      "name": "Decorators/Annotations",
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": { "foreground": "#c084fc" }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": { "foreground": "#a78bfa" }
    },
    {
      "name": "Markdown headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#a78bfa", "fontStyle": "bold" }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold"],
      "settings": { "foreground": "#f0f0f5", "fontStyle": "bold" }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic"],
      "settings": { "foreground": "#f0f0f5", "fontStyle": "italic" }
    },
    {
      "name": "Markdown code",
      "scope": ["markup.raw", "markup.inline.raw"],
      "settings": { "foreground": "#86efac" }
    },
    {
      "name": "Markdown links",
      "scope": ["markup.underline.link"],
      "settings": { "foreground": "#67e8f9" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ef4444" }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": { "foreground": "#8888a0" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": "#fde68a",
    "interface": "#fde68a",
    "type": "#fde68a",
    "enum": "#fde68a",
    "enumMember": "#fb923c",
    "function": "#67e8f9",
    "method": "#67e8f9",
    "variable.readonly": "#f0f0f5",
    "parameter": "#f0f0f5",
    "property": "#67e8f9",
    "keyword": "#a78bfa",
    "string": "#86efac",
    "number": "#fb923c",
    "comment": {"foreground": "#4a4a60", "fontStyle": "italic"},
    "namespace": "#fde68a",
    "decorator": "#c084fc",
    "macro": "#a78bfa"
  }
}

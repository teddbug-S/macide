  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "comment.block", "comment.line", "punctuation.definition.comment"],
      "settings": { "foreground": "#4a4a60", "fontStyle": "italic" }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.control", "keyword.operator", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#a78bfa" }
    },
    {
      "name": "Control flow",
      "scope": ["keyword.control.flow", "keyword.control.return", "keyword.control.import", "keyword.control.export"],
      "settings": { "foreground": "#a78bfa", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template", "string.interpolated"],
      "settings": { "foreground": "#86efac" }
    },
    {
      "name": "String punctuation",
      "scope": ["punctuation.definition.string"],
      "settings": { "foreground": "#86efac" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.numeric.integer", "constant.numeric.float", "constant.numeric.hex"],
      "settings": { "foreground": "#fb923c" }
    },
    {
      "name": "Boolean & null",
      "scope": ["constant.language.boolean", "constant.language.null", "constant.language.undefined"],
      "settings": { "foreground": "#fb923c", "fontStyle": "italic" }
    },
    {
      "name": "Built-in constants",
      "scope": ["constant.language", "variable.language.this", "variable.language.self", "variable.language.super"],
      "settings": { "foreground": "#fb923c" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function-call.generic", "support.function"],
      "settings": { "foreground": "#67e8f9" }
    },
    {
      "name": "Function parameters",
      "scope": ["variable.parameter", "meta.function.parameters"],
      "settings": { "foreground": "#f0f0f5", "fontStyle": "italic" }
    },
    {
      "name": "Classes & types",
      "scope": ["entity.name.class", "entity.name.type", "entity.name.namespace", "entity.name.struct", "entity.name.enum"],
      "settings": { "foreground": "#fde68a" }
    },
    {
      "name": "Type annotations",
      "scope": ["entity.other.inherited-class", "meta.type.annotation", "support.type"],
      "settings": { "foreground": "#fde68a" }
    },
    {
      "name": "Interfaces",
      "scope": ["entity.name.interface"],
      "settings": { "foreground": "#06b6d4" }
    },
    {
      "name": "Decorators / annotations",
      "scope": ["meta.decorator", "punctuation.decorator", "storage.type.annotation"],
      "settings": { "foreground": "#a78bfa", "fontStyle": "italic" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#f0f0f5" }
    },
    {
      "name": "Object properties",
      "scope": ["variable.other.property", "support.variable.property", "meta.object.member"],
      "settings": { "foreground": "#67e8f9" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation.separator", "punctuation.accessor"],
